                    xmlns:converter="clr-namespace:Gymdata.Accreditation.Converter">
    <SolidColorBrush Color="#F1F3F4" x:Key="ControlColor"/>
    <SolidColorBrush Color="#DEE1E6" x:Key="DarkControlColor"/>
    <SolidColorBrush Color="#40454D" x:Key="MenuIconColor"/>
    <SolidColorBrush Color="#CCCCCC" x:Key="BorderColor"/>
    <SolidColorBrush Color="#999999" x:Key="DarkBorderColor"/>

    <NullableBoolConverter x:Key="NullableBoolConverter"/>
